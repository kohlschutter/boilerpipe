require(["istats-1"], function (istats) {
    (function() {
        var selectionPercentage = 100;
        var beta_cookie = {
                name: "ckps_tablet_beta",
                optInValue: "i",
                optOutValue: "o",
                betaParam: "tablet-beta"
            },
            preference_cookie = {
                name: "ckps_d",
                tabletValue: "t"
            },
            page_elements = {
                banner: "opt-in-promo",
                bannerDismiss: "opt-in-promo__dismiss",
                bannerFixed: "opt-in-promo--fixed",
                bannerOptIn: "opt-in-promo__opt-in",
                bannerVisible: "opt-in-promo--visible"
            },
            twoHeadedCoin = "two-headed-coin",

            getPromo = function() {
                return document.getElementById(page_elements.banner);
            },
            showPromo = function () {
                getPromo().className += " " + page_elements.bannerVisible;
            },
            optIn = function (e) {
                stopEventPropagation(e);
                setCookie(this.beta_cookie.name, this.beta_cookie.optInValue);
                setCookie(this.preference_cookie.name, this.preference_cookie.tabletValue);
                istats.log('click', 'take-me-there');
                redirectToBetaDomain();
            },
            optOut = function () {
                setCookie(this.beta_cookie.name, this.beta_cookie.optOutValue);
                istats.log('click', 'no-thanks');
                makeNonScrolly();
            },
            makeScrolly = function () {
                getPromo().className += " " + page_elements.bannerFixed;
            },
            makeNonScrolly = function () {
                var promoClasses = getPromo().className;
                promoClasses = promoClasses.replace(page_elements.bannerFixed, "");
                getPromo().className = promoClasses;
            },
            redirectToBetaDomain = function () {
                if (this.window.location.hostname.match(/bbc\.com/)) {
                    this.window.location = 'http://m.bbc.com/news';
                }
                else {
                    this.window.location = 'http://m.bbc.co.uk/news';
                }
            },
            getBetaParams = function () {
                var match = RegExp('[?&]' + beta_cookie.betaParam + '=([^&]*)').exec(window.location.search);
                return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
            },
            coinIsTwoHeaded = function () {
                return RegExp('[?&]' + twoHeadedCoin).exec(window.location.search) ? true : false;
            },
            hasOptedOutOfBeta = function () {
                return beta_cookie.optOutValue === (getBetaCookieValue() || getBetaParams());
            },
            isSelectable = function () {
                return (
                    ((this.clientCutsTheMustard() || this.clientIsIE9()) && this.clientIsSelected())
                    || this.coinIsTwoHeaded()
                    );
            },
            setCookie = function (cookieToSet, valueToSet, days) {

                days = days || 120;
                var path = "/";
                var domain = window.location.hostname.match(/com/) ? ".bbc.com" : ".bbc.co.uk";
                var date = new Date();
                date.setTime(date.getTime() + days*24*60*60*1000);
                var expires = "; expires=" + date.toGMTString();
                document.cookie = cookieToSet + "=" + valueToSet + expires + "; path=" + path + "; domain=" + domain;
            },
            getBetaCookieValue = function () {
                var cookies = document.cookie.split(";");
                var optInCookie;
                var optInCookieValue;

                var cookieIndex = cookies.length;

                while(cookieIndex--) {
                    if(cookies[cookieIndex].indexOf(beta_cookie.name) > -1) {
                        optInCookie = cookies[cookieIndex];
                        optInCookieValue = optInCookie.split('=').pop();

                        return optInCookieValue;
                    }
                }
            },
            clientCutsTheMustard = function () {
                return (
                    'querySelector' in this.doc && 'localStorage' in this.win && 'addEventListener' in this.win
                    );
            },
            clientIsIE9 = function () {
                return window.navigator.userAgent.match(/Trident\/5/) ? true : false;
            },
            clientIsSelected = function () {
                return (Math.random() < (selectionPercentage / 100)) ? true : false;
            },
            setupEvents = function () {
                this.attachEventToElement(this.doc.getElementById(page_elements.bannerDismiss), 'click', (function (scope) {
                    return function () {
                        return scope.optOut.apply(scope, arguments);
                    };
                })(this));
                this.attachEventToElement(this.doc.getElementById(page_elements.bannerOptIn), 'click', (function (scope) {
                    return function () {
                        return scope.optIn.apply(scope, arguments);
                    };
                })(this));
            },
            attachEventToElement = function (elem, event, fn) {
                if (elem.addEventListener) {
                    elem.addEventListener(event, fn, false);
                } else {
                    elem.attachEvent("on" + event, (function (scope) {
                        return function () {
                            return(fn.call(elem, scope.win.event));
                        };
                    })(this));
                }
            },
            stopEventPropagation = function(e) {
                if (e.stopPropagation) {
                    e.stopPropagation();
                } else {
                    e.cancelBubble = true;
                }
                if (e.preventDefault) {
                    e.preventDefault();
                } else {
                    e.returnValue = false;
                }
            },
            execute = function() {
                if (this.isSelectable()) {
                    this.setupEvents();
                    this.showPromo();

                    if (hasOptedOutOfBeta()) {
                        makeNonScrolly();
                    }
                }
            };

        return {
            clientCutsTheMustard: clientCutsTheMustard,
            clientIsIE9: clientIsIE9,
            clientIsSelected: clientIsSelected,
            coinIsTwoHeaded: coinIsTwoHeaded,
            hasOptedOutOfBeta: hasOptedOutOfBeta,
            isSelectable: isSelectable,
            showPromo: showPromo,
            execute: execute,
            setupEvents: setupEvents,
            optIn: optIn,
            beta_cookie: beta_cookie,
            preference_cookie: preference_cookie,
            page_elements: page_elements,
            redirectToBetaDomain: redirectToBetaDomain,
            setCookie: setCookie,
            getBetaCookieValue: getBetaCookieValue,
            makeScrolly: makeScrolly,
            makeNonScrolly: makeNonScrolly,
            optOut: optOut,
            attachEventToElement: attachEventToElement,
            win: window,
            doc: document,
            nav: navigator,
            getPromo: getPromo,
            getBetaParams: getBetaParams,
            stopEventPropagation: stopEventPropagation
        };
    })().execute();
});
