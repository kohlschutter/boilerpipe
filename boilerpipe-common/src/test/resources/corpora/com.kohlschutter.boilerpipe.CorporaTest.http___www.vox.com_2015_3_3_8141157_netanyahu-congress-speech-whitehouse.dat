  <script type="script/template" id="explainers-card-template">
  <div class="m-explainer__card-wrapper">
    <div class="m-explainer__pager">
      <a class="m-explainer__pager-control previous" href="[%= data.prevUrl || '#' %]">&lt;</a>
      [% if (data.position && data.total) { %]
        <p class="m-explainer__pager__card-count">Card [%= data.position %] of [%= data.total %]</p>
      [% } %]
      <a class="m-explainer__pager-control next" href="[%= data.nextUrl || '#' %]">&gt;</a>
    </div>

    <div class="m-explainer__card-content">
      [% if (data.title) { %]
        <h2 class="m-explainer__card__headline">[%= data.title %]</h2>
      [% } %]
      [%= data.body || '<div class="m-explainer__card__spinner"></div>' %]
    </div>

    <div class="m-explainer__card__lower">
      <div class="m-explainer__pager lower">
        <a class="m-explainer__pager-control previous" href="[%= data.prevUrl %]">&lt;</a>

      [% if (data.shortUrl && data.shareUrl) { %]
        <div class="m-explainer__card-social" data-analytics-placement="card">
        Share <span class="this">This</span> Card
          <a class="m-explainer__social facebook" href="https://www.facebook.com/sharer/sharer.php?u=[%= data.shareUrl %]" onclick="window.open(this.href, '', 'height=279,width=575'); event.preventDefault();" data-analytics-social="facebook">Facebook</a>
          <a class="m-explainer__social twitter" href="https://twitter.com/intent/tweet?url=[%= data.shareUrl %]&text=[%= data.title %]" data-analytics-social="twitter">twitter</a>
          <a class="m-explainer__social google_plus" href="https://plus.google.com/share?url=[%= data.shortUrl %]" onclick="window.open(this.href, '', 'height=279,width=575'); event.preventDefault();" data-analytics-social="gplus">google plus</a>
          <a class="m-explainer__social print" href="javascript:window.print()" data-analytics-class="print">print</a>
        </div>
      [% } %]

        <a class="m-explainer__pager-control next" href="[%= data.nextUrl %]">&gt;</a>
      </div>
    </div>
  </div>
</script> 
