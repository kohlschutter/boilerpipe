  <script type="script/template" id="explainers-explore-results-template">
[% if (data.cardStacks && data.cardStacks.length > 0) { %]
  [% data.cardStacks.each(function(cardStack) { %]
    <a class="m-explainer-explore__explainer" href="[%= cardStack.get('url') %]">
      <div class="m-explainer-explore__explainer__stack"></div>
      <div class="m-explainer-explore__explainer__head">
        <div class="m-explainer-explore__explainer__head__count">[%= cardStack.get('cardsCount') %] cards</div>
      </div>
      <div class="m-explainer-explore__explainer__body">
        <h2 class="m-explainer-explore__explainer__title">[%= cardStack.get('title') %]</h2>
        <h3 class="m-explainer-explore__explainer__time">Updated [%= cardStack.get('updatedAt') %]</h3>
      </div>
    </a>
  [% }) %]
[% } else { %]
  <div class="m-explainer-explore__no-results">
    <div class="m-explainer-explore__no-results__sorry">
      <h2>Sorry! There’s no card stack with that term.</h2>
      <p>If that seems impossible, please czech your spelling.</p>
    </div>
    <div class="m-explainer-explore__no-results__submit">
      <p>If you think there should be a card stack for this term, click the submit button to send in your suggestion.</p>
      <button>Submit!</button>
      <p>If you’d like to send a more detailed suggestion, <a href="mailto:editors@vox.com?subject=Card stack submission">contact us here</a>. Thanks!</p>
    </div>
  </div>
  [% if (data.suggestedCardStacks && data.suggestedCardStacks.length > 0) { %]
    <div class="m-explainer-explore__break">
      <div class="m-explainer-explore__break__text">Or try one of these!</div>
    </div>
    [% data.suggestedCardStacks.each(function(cardStack) { %]
      <a class="m-explainer-explore__explainer" href="[%= cardStack.get('url') %]">
        <div class="m-explainer-explore__explainer__stack"></div>
        <div class="m-explainer-explore__explainer__head">
          <div class="m-explainer-explore__explainer__head__count">[%= cardStack.get('cardsCount') %] cards</div>
        </div>
        <div class="m-explainer-explore__explainer__body">
          <h2 class="m-explainer-explore__explainer__title">[%= cardStack.get('title') %]</h2>
          <h3 class="m-explainer-explore__explainer__time">Updated [%= cardStack.get('updatedAt') %]</h3>
        </div>
      </a>
    [% }) %]
  [% } %]
[% } %]
</script> 
